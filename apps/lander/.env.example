# ============================================================
# Lander Environment Variables
# ============================================================
# PUBLIC_ prefix = available at build time in Astro templates.
# Copy this to .env and fill in values.
# ============================================================

# Site configuration
SITE_URL=https://yourdomain.com
PUBLIC_SITE_NAME=Personal Loans
PUBLIC_COMPANY_NAME=LoanConnect

# Account identification
PUBLIC_ACCOUNT_ID=acct_001

# Tracking endpoints
PUBLIC_TRACK_URL=https://api.yourdomain.com/track

# Voluum tracking domain (for impression pixel)
PUBLIC_VOLUUM_DOMAIN=your-voluum-tracking-domain.com

# LeadsGate form ID
PUBLIC_LEADSGATE_FORM_ID=your-form-id-here
